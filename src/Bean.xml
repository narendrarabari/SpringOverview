<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean name="user" class="com.nr.entities.User">
	<property name="firstName" value="Narendra"/>
		<property name="lastName" value="Rabari"/>
		<property name="addressObj" ref="address"></property>
	</bean>
	
	<bean name="address" class="com.nr.entities.Address">
	<property name="address1" value="abc"/>
	<property name="address2" value="xyz"/>
	</bean>
	
</beans>
